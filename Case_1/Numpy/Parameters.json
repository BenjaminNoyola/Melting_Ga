{
    "steps" : {
        "type" : "int",
        "help" : "Edit this line to change the time steps",
        "default" : 300000},

    "lattice_x" : {
        "type": "int",
        "help": "lattices in x direction; edit this line to change the size",
        "default": 256},

    "lattice_y" : {
        "type": "int",
        "help": "lattices in y direction; edit this line to change the size",
        "default": 8},

    "rho" : {
        "type": "float",
        "help": "density of the PCM",
        "default": 1.0},

    "Delta_alfa" : {
        "type": "float",
        "help": "thermal diffusivity ratio",
        "default": 10.0},

    "T_i" : {
        "type": "float",
        "help": "low temperature",
        "default": -1.0},

    "T_b" : {
        "type": "float",
        "help": "high temperature",
        "default": 1.0},

    "T_m" : {
        "type": "float",
        "help": "melting temperature",
        "default": 0.0},

    "T_s" : {
        "type": "float",
        "help": "temperatura where solid starts",
        "default": -0.02},

    "T_l" : {
        "type": "float",
        "help": "temperature where liquid starts",
        "default": 0.02},


    "alpha_s" : {
        "type": "float",
        "help": "thermal diffusivity of solid",
        "default": 0.002},

    "poros" : {
        "type": "float",
        "help": "porosity",
