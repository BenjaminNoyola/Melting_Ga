        "default": -2.0},

    "St" : {
        "type": "float",
        "help": "Stefan number",
        "default": 1.0},

    "F_0" : {
        "type": "float",
        "help": "Fourier number",
        "default": 0.01},

    "H" : {
        "type": "float",
